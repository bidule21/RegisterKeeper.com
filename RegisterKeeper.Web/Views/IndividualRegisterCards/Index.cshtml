@model IEnumerable<RegisterKeeper.Web.Models.IndividualRegisterCard>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@*<p>
    @Html.ActionLink("Create New", "CreateForIndividualCompetition")
</p>*@
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CompetitorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompetitorIndexNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompetitorClass)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompetitorClub)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CompetitorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompetitorIndexNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompetitorClass)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompetitorClub)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditForIndividualCompetition", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
